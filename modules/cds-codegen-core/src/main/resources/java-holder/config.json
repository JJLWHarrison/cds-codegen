{
    "handlers": [
        {
            "type": "SectionHandler",
            "baseDirectory": "src/gen/java",
            "templateFile": "java-holder/section/interface.vm",
            "fileName": "${cds.getSectionName()}.java",
            "filePath": "${cds.getPackageName().replace(\".\", \"/\")}/api",
            "additionalAttributes": {
                "packageName": "${cds.getPackageName()}.api"
            }
        },   
        {
            "type": "SectionHandler",
            "baseDirectory": "src/gen/java",
            "templateFile": "java-holder/section/impl.vm",
            "fileName": "${cds.getSectionName()}Controller.java",
            "filePath": "${cds.getPackageName().replace(\".\", \"/\")}/api",
            "additionalAttributes": {
                "packageName": "${cds.getPackageName()}.api"
            }
        },   
        {
            "type": "SectionHandler",
            "baseDirectory": "src/gen/java",
            "templateFile": "java-holder/section/dataloader.vm",
            "fileName": "${cds.getSectionName()}DataLoder.java",
            "filePath": "${cds.getPackageName().replace(\".\", \"/\")}/util",
            "additionalAttributes": {
                "packageName": "${cds.getPackageName()}.util"
            }
        },               
        {
            "type": "DataDefinitionHandler",
            "baseDirectory": "src/gen/java",
            "modelTemplate": "java-holder/model/model.vm",
            "enumTemplate": "java-holder/model/enum.vm",
            "fileName": "${cds.getDefinitionName()}.java",
            "filePath": "${cds.getPackageName().replace(\".\", \"/\")}/model",
            "additionalAttributes": {
                "packageName": "${cds.getPackageName()}.model"
            }
        },
        {
            "type": "DataDefinitionHandler",
            "baseDirectory": "src/gen/java",
            "modelTemplate": "java-holder/model/repository.vm",
            "fileName": "${cds.getDefinitionName()}Repository.java",
            "filePath": "${cds.getPackageName().replace(\".\", \"/\")}/repository",
            "additionalAttributes": {
                "packageName": "${cds.getPackageName()}.repository"
            }
        },                     
        {
            "type": "GenericHandler",
            "baseDirectory": "src/gen/java",
            "fileMapping": {
                "java-holder/static/HolderApplication.vm": "au/org/consumerdatastandards/holder/HolderApplication.java",
                "java-holder/static/HomeController.vm": "au/org/consumerdatastandards/holder/configuration/HomeController.java",
                "java-holder/static/OpenAPIDocumentationConfig.vm": "au/org/consumerdatastandards/holder/configuration/OpenAPIDocumentationConfig.java",
                "java-holder/static/ContextEventListener.vm": "au/org/consumerdatastandards/holder/util/ContextEventListener.java",
                "java-holder/static/WebUtil.vm": "au/org/consumerdatastandards/holder/util/WebUtil.java"
            },
            "additionalAttributes": {
                "packageName": "${cds.getPackageName()}"
            }            
        }
    ],
    "typeMappings": {
            "String": "String",
            "List": "List<%s>",
            "DateTime": "DateTime",
            "Date": "Date",
            "URI": "URI"
    }
}