<?xml version="1.0" encoding="UTF-8"?>
<Configuration status="ERROR">
    <Appenders>
        <Console name="Console" target="SYSTEM_OUT">
            <PatternLayout pattern="%d{HH:mm:ss.SSS} [%t] %-5level %logger{36} - %msg%n"/>
        </Console>
    </Appenders>
    <Loggers>
        <Root level="ERROR">
            <AppenderRef ref="Console"/>
        </Root>
        <Logger name="org.reflections.Reflections" level="ERROR">
            <AppenderRef ref="Console"/>
        </Logger>
        <Logger name="au.org.consumerdatastandards.codegen.generator.code.CodeGenerator" level="DEBUG">
            
        </Logger>
        <Logger name="au.org.consumerdatastandards.codegen.generator.code.handler.datadefinition.DataDefinitionHandler" level="DEBUG">
            
        </Logger>
        <Logger name="au.org.consumerdatastandards.codegen.util.ModelCodegenConverter" level="DEBUG">
            
        </Logger>
        
    </Loggers>
</Configuration>
