{
    "handlers": [
        {
            "type": "DataDefinitionHandler",
            "baseDirectory": "src/gen/java",
            "modelTemplate": "java/model.vm",
            "enumTemplate": "java/enum.vm",
            "fileName": "${cds.getDefinitionName()}.java",
            "filePath": "$cds.getPackageName().replace(\".\", \"/\")",
            "additionalAttributes": {
                "packageName": "$cds.getPackageName()"
            }
        },
        {
            "type": "SectionHandler",
            "baseDirectory": "src/gen/java",
            "sectionTemplate": "java/api.vm",
            "fileName": "${cds.getInterfaceName()}.java",
            "filePath": "au/org/consumerdatastandards/api",
            "additionalAttributes": {
                "packageName": "$cds.getPackageName()"
            }
        }
    ],
    "typeMappings": {
            "String": "String",
            "List": "List<%s>",
            "DateTime": "DateTime",
            "Date": "Date",
            "URI": "URI"
    }
}