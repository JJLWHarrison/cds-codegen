{
    "handlers": [
        {
            "type": "DataDefinitionHandler",
            "baseDirectory": "src/gen/java",
            "templateName": "java/model.vm",
            "fileName": "${cds.getDefinitionName()}.java",
            "filePath": "$cds.getPackageName().replace(\".\", \"/\")",
            "additionalAttributes": {
                "packageName": "$cds.getPackageName()"
            }
        }
    ],
    "typeMappings": {
            "String": "String",
            "List": "List<%s>"
    }
}